<% if @modules.empty? -%>
Fabricator(:<%= @resource_name %>) do
<% else -%>
Fabricator(:<%= @resource_name %>, class_name: '<%= @modulized_resource_class_name -%>') do
<% end -%>
<%   @fields.each_pair do |field_name, kind| -%>
<%   if kind == 'belongs_to' -%>
  <%= field_name %>
<%   elsif kind == 'text' -%>
  <%= field_name -%> { Faker::Lorem.sentence }
<%   elsif kind == 'string' -%>
  <%= field_name -%> { Faker::Lorem.word }
<%   end -%>
<%   end -%>
end
